public class test{
 public static void main(String [] args) {
   
        int num1=1;
        int num2=110;
        twisters (num1,num2);
 }
  public static void twisters(int num1,int num2) {

       for( ;num1<=num2;num1++){ 

       if (num1 %2==0)
        System.out.print(" Tweetle ");
       
       else if (num1%4==0) 
        System.out.print(" Beetle ");
       
       else if (num1%6==0)
        System.out.print(" Poodle ");
       
        else
             System.out.print(num1);
        
        if (num1%11==0){
        System.out.println();
       }
    }

}
}
        
    